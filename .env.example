# Western Australia Emergency Dashboard Environment Variables

# Mapbox Configuration
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

# QGIS API Configuration
VITE_QGIS_API_URL=http://localhost:8000/api
VITE_QGIS_API_KEY=your_qgis_api_key_here

# Application Configuration
VITE_APP_ENVIRONMENT=development
VITE_ENABLE_MOCK_DATA=true
VITE_API_BASE_URL=http://localhost:3001

# Emergency Data Configuration
VITE_DATA_REFRESH_INTERVAL=900000
VITE_CACHE_DURATION=300000

# Geographic Configuration
VITE_DEFAULT_CENTER_LAT=-31.9505
VITE_DEFAULT_CENTER_LNG=115.8605
VITE_DEFAULT_ZOOM=6

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=false
VITE_ENABLE_DEBUG_MODE=false
