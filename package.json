{
  "name": "wa-emergency-dashboard",
  "author": "Emergency Dashboard Team",
  "description": "Western Australia Emergency Dashboard - Interactive map for bushfire and flood emergency information",
  "keywords": [
    "emergency",
    "dashboard",
    "western-australia",
    "bushfire",
    "flood",
    "qgis",
    "mapbox",
    "react",
    "vite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qinscode/responda"
  },
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "git init && npx husky init && npx playwright install && shx rm .husky/pre-commit",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --max-warnings 0 --fix",
    "dev": "vite",
    "test": "vitest run src/ && playwright test",
    "test:unit": "vitest src/",
    "test:unit:coverage": "vitest --coverage src/",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.6",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-router": "^1.130.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.539.0",
    "mapbox-gl": "^3.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.6.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.10",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@playwright/test": "^1.54.1",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/router-devtools": "^1.130.0",
    "@tanstack/router-plugin": "^1.130.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "shx": "^0.4.0",
    "tailwindcss": "^3.4.14",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "7.0.6",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
